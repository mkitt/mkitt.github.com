{
  "name": "mkitt.net",
  "description": "Web and mobile app developer specializing in modular design systems, front-end architecture, product development & team management.",
  "homepage": "https://mkitt.net/",
  "private": true,
  "version": "6.6.6",
  "license": "MIT",
  "scripts": {
    "build": "next build && next export -o build",
    "postbuild": "mv ./build/index.html ./ && mv ./build/404.html ./ && mv ./build/resume/index.html ./resume/ && rm -r ./build",
    "dev": "next",
    "lint": "eslint ./"
  },
  "dependencies": {
    "@emotion/core": "10.0.7",
    "@emotion/styled": "10.0.7",
    "next": "8.0.1",
    "react": "16.8.3",
    "react-dom": "16.8.3",
    "react-markdown": "4.0.6"
  },
  "devDependencies": {
    "babel-plugin-emotion": "10.0.7",
    "eslint": "5.14.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4"
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      [
        "emotion"
      ]
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": [
      "airbnb"
    ],
    "rules": {
      "react/jsx-filename-extension": "off",
      "react/jsx-key": "error",
      "react/prop-types": "off",
      "semi": [
        "error",
        "never"
      ]
    }
  }
}
